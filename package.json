{
  "name": "makerspace",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "npm run db:build && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db": "npx prisma db push && npx prisma db seed",
    "db:nix": "prisma db push && prisma db seed",
    "db:build": "npx prisma db push",
    "sb-stop": "supabase stop --backup",
    "sb-restart": "supabase stop --backup && supabase start",
    "sb-reset": "supabase db reset && yarn sb-restart && prisma db push"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tiptap/extension-placeholder": "^2.2.3",
    "@types/eslint": "8.56.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "fluent-svelte": "^1.6.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.69.6",
    "supabase": "1.112.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.3",
    "sveltekit-superforms": "^2.9.0",
    "tslib": "^2.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.3",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "5.4.1",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.39.2",
    "@tiptap/core": "^2.2.3",
    "@tiptap/extension-underline": "^2.2.3",
    "@tiptap/pm": "^2.2.3",
    "@tiptap/starter-kit": "^2.2.3",
    "nanoid": "^5.0.4"
  }
}
