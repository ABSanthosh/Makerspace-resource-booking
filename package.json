{
	"name": "makerspace",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "npm run db-build && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db": "prisma db push && prisma db seed && npx tsx ./src/lib/seedTrigger.ts",
		"db-build": "npx prisma db push && npx prisma db seed && npx tsx ./src/lib/seedTrigger.ts",
		"sb-stop": "supabase stop --backup",
		"sb-restart": "supabase stop --backup && supabase start",
		"sb-reset": "supabase db reset && yarn sb-restart && prisma db push"
	},
	"prisma": {
		"seed": "tsx ts-node --esm prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/node": "^20.11.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.3.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postgres": "^3.4.3",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.69.6",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"sveltekit-superforms": "^1.13.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.3",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "5.4.1",
		"@supabase/ssr": "^0.0.10",
		"@supabase/supabase-js": "^2.39.2"
	}
}
